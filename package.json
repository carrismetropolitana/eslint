{
    "name": "@carrismetropolitana/eslint",
    "type": "module",
    "version": "2024.5.6151",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist/"
    ],
    "scripts": {
        "start": "tsc && node build/index.js",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
