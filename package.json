    "dependencies": {
        "@eslint/js": "9.2.0",
        "@stylistic/eslint-plugin": "1.8.0",
        "eslint": "8.57.0",
        "globals": "^15.1.0",
        "typescript-eslint": "7.8.0"
    },
    "devDependencies": {
        "@types/eslint__js": "^8.42.3",
